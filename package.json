{
  "name": "react-ts-app-starter",
  "version": "1.3.0",
  "description": "Quickly setup a React app.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "build:dev": "tsc --watch",
    "lint": "eslint src/**/*.ts",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "git add -A src",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "React",
    "starter",
    "boilerplate"
  ],
  "author": "Alexander Nedomansky <anedomansky@icloud.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://git@github.com/anedomansky/react-ts-app-starter.git"
  },
  "bugs": {
    "url": "https://github.com/anedomansky/react-ts-app-starter/issues"
  },
  "homepage": "https://github.com/anedomansky/react-ts-app-starter#readme",
  "devDependencies": {
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "eslint": "^8.1.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-security": "^1.4.0",
    "typescript": "^4.4.4"
  },
  "files": [
    "dist/**/*"
  ]
}
